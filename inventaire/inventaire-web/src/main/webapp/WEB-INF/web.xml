<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="UniLibraryWeb" version="3.0">
  <display-name>UniLibraryWeb</display-name>
  <servlet>
    <servlet-name>categories</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletListCategories</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>categorycreate</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletCategoryCreateModify</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>categorymodify</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletCategoryCreateModify</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>categorysave</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletCategorySave</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>categorydelete</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletCategoryDelete</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>titles</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletSearchTitles</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>titledetail</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletTitleDetail</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>titlemodify</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletTitleModify</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>titlecreate</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletTitleCreate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>booksave</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletBookSave</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>audiosave</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletAudioRecordSave</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>titledelete</servlet-name>
    <servlet-class>fr.uni.institute.library.web.ServletTitleDelete</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>categories</servlet-name>
    <url-pattern>/categories.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>categorycreate</servlet-name>
    <url-pattern>/categorycreate.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>categorymodify</servlet-name>
    <url-pattern>/categorymodify.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>categorysave</servlet-name>
    <url-pattern>/categorysave.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>categorydelete</servlet-name>
    <url-pattern>/categorydelete.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>titles</servlet-name>
    <url-pattern>/titles.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>titledetail</servlet-name>
    <url-pattern>/titledetail.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>titlemodify</servlet-name>
    <url-pattern>/titlemodify.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>titlecreate</servlet-name>
    <url-pattern>/titlecreate.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>booksave</servlet-name>
    <url-pattern>/booksave.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>audiosave</servlet-name>
    <url-pattern>/audiosave.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>titledelete</servlet-name>
    <url-pattern>/titledelete.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <resource-ref>
    <description>DataSource</description>
    <res-ref-name>jdbc/UniLibraryDS</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
</web-app>